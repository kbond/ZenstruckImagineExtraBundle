{
    "name": "zenstruck/imagine-extra-bundle",
    "description": "Integrate imagine-filters library into Symfony2",
    "keywords": ["imagine"],
    "homepage": "http://zenstruck.com/project/ZenstruckImagineExtraBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "symfony/http-kernel": "~2.1",
        "symfony/dependency-injection": "~2.1",
        "zenstruck/imagine-filters" : "~1.2"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "~0.4"
    },
    "suggest": {
        "liip/imagine-bundle": "For integration with LiipImagineBundle",
        "avalanche123/imagine-bundle": "For integration with AvalancheImagineBundle"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": { "Zenstruck\\Bundle\\ImagineExtraBundle\\": "" }
    }
}
