<?xml version="1.0" encoding="utf-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="zenstruck_imagine_extra.filter.loader.liip.square.class">Zenstruck\Bundle\ImagineExtraBundle\Imagine\Liip\SquareThumbnailFilterLoader</parameter>
    </parameters>

    <services>
        <service id="zenstruck_imagine_extra.filter.loader.liip.square" class="%zenstruck_imagine_extra.filter.loader.liip.square.class%">
            <tag name="liip_imagine.filter.loader" loader="square_thumbnail" />
        </service>
    </services>
</container>
